



function specialconcat(input, delimiter) {
   if (Array.isArray(input)) {
      var data = '';
      var delim = delimiter == undefined ? "" : delimiter
      for (var i = 0; i < input.length; i++) {
         data += input[i] + delim;
      }
      return delimiter == undefined ? data : data.substring(0, data.length - 1)
   } else {
      return input
   }



function sortResponses() {
var bank = SpreadsheetApp.getActive().getSheetByName("bank");
var bookmark = SpreadsheetApp.getActive().getSheetByName("bookmark");
var co = SpreadsheetApp.getActive().getSheetByName("co");
co.sort(1, false);
  bank.sort(1, false);
bookmark.sort(1, false);
}
